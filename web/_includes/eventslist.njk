<p>Events</p>
<ol reversed class="eventlist" style="counter-reset: start-from {{ eventslist.length + 1 }}">
  {% for event in eventslist | reverse %}
    {% set currentevent = event.data.event %}
    <li class="eventlist-item{% if event.url == url %} eventlist-item-active{% endif %}">
      <a href="{{ event.url | url }}" class="eventlist-link">
        {% if currentevent.title %}{{ currentevent.title }}
        {% else %}
          <code>{{ event.url }}</code>
        {% endif %}
      </a>
      <time class="eventlist-date" datetime="{{ currentevent.publishedAt }}">{{ currentevent.publishedAt|readableDate }}</time>
    </li>
  {% endfor %}
</ol>